<Application x:Class="WpfClient.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WpfClient"
             xmlns:viewModel ="clr-namespace:WpfClient.MVVM.ViewModel" xmlns:view="clr-namespace:WpfClient.MVVM.View">
    <Application.Resources>
        <DataTemplate DataType="{x:Type TypeName=viewModel:HomeViewModel}">
            <view:HomeView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type TypeName=viewModel:LogInViewModel}">
            <view:LogInView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type TypeName=viewModel:SearchClientViewModel}">
            <view:SearchClientView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type TypeName=viewModel:RegisterClientViewModel}">
            <view:RegisterClientView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type TypeName=viewModel:SideBarViewModel}">
            <view:SideBarView/>
        </DataTemplate>
    </Application.Resources>
</Application>
