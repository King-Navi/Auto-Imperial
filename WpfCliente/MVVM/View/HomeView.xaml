<UserControl x:Class="WpfCliente.MVVM.View.HomeView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfCliente.MVVM.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="Pink">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid Grid.Column="0" Background="Green">
            <Button Content="Log out" HorizontalAlignment="Center" Margin="0,304,0,0" VerticalAlignment="Top" Width="90" Height="25" Command="{Binding NavegateToLogInViewCommand}"/>
        </Grid>
        <Grid Grid.Column="1" Background="Aqua">
            <ContentControl Content="{Binding InternalNavigationService.CurrentView}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
        </Grid>
    </Grid>
</UserControl>
